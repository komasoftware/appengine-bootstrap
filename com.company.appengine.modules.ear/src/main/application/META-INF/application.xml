<?xml version="1.0"?>
<!-- This is the application.xml file intended for use with gradle, if you find it in your maven project, then that's a problem most likely. -->
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd"
             version="6">
    <display-name>com.company.appengine.modules.ear</display-name>
    <module>
        <web>
            <web-uri>com.company.endpoints</web-uri>
            <context-root>com.company.endpoints</context-root>
        </web>
    </module>
    <library-directory>lib</library-directory>
</application>
